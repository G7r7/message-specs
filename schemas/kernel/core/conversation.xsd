<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
<xsd:include schemaLocation="version.xsd"/>
<xsd:include schemaLocation="message.xsd"/>
  <xsd:element name="conv">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="msg"/>
      </xsd:sequence>
      <xsd:attribute ref="version" use="required"/>
      <xsd:attribute name="id" use="required" type="xsd:positiveInteger"/>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
